{
  "name": "step-fns",
  "version": "0.16.0",
  "description": "This library is intended to help projects intending to build a \"micro-service architecture\" to achieve those ambitions with some handy orchestration classes and types.",
  "homepage": "https://github.com/inocan-group/aws-orchestrate#readme",
  "bugs": {
    "url": "https://github.com/inocan-group/aws-orchestrate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/aws-orchestrate.git"
  },
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "typings": "dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "autoindex": "yarn do autoindex",
    "build": "run-s clean autoindex build:bundle",
    "build:analyze": "node devops/build es2020 --analyze",
    "build:bundle": "node devops/build es2020 --closure --analyze",
    "build:cjs": "tsc -P tsconfig.cjs.json",
    "build:es": "tsc -P tsconfig.es.json",
    "build:tsc": "run-p build:es build:cjs",
    "circular": "yarn madge --circular src/**/*.ts",
    "clean": "rimraf dist",
    "clean:nuclear": "yarn rimraf node_modules yarn.lock && yarn cache clean && yarn && yarn upgrade",
    "docs": "node_modules/.bin/vuepress dev docs",
    "docs:build": "node_modules/.bin/vuepress build docs",
    "lint": "run-s lint:src lint:test lint:tsc",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:test": "eslint test/**/*.ts --fix",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "visualize": "yarn madge src --ts-config tsconfig.json --extensions ts -i /tmp/dep-visualize.png && open /tmp/dep-visualize.png",
    "watch": "yarn run-p watch:esnext watch:cjs",
    "watch:cjs": "yarn tsc -P tsconfig.cjs.json -w",
    "watch:esnext": "yarn tsc -P tsconfig.esnext.json -w"
  },
  "dependencies": {
    "aws-log": "^0.11.1"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
    "@jest/types": "^26.6.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/jest": "^26.0.22",
    "@types/js-yaml": "4",
    "@types/mermaid": "8",
    "@types/node": "14",
    "@types/rimraf": "^3.0.0",
    "@types/test-console": "^1.1.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-last-updated": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "@vuepress/plugin-pwa": "^1.8.2",
    "async-shelljs": "^0.1.4",
    "aws-sdk": "2",
    "aws-ssm": "^0.3.13",
    "aws-xray-sdk-core": "^3.3.1",
    "common-types": "^1.23.1",
    "do-devops": "^0.6.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "inferred-types": "^0.3.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "madge": "^4.0.2",
    "mermaid": "^8.9.2",
    "native-dash": "^1.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "test-console": "^1.1.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3",
    "vuepress": "^1.8.2",
    "vuepress-plugin-autometa": "^0.1.13",
    "vuepress-plugin-mermaidjs": "^1.8.0"
  },
  "peerDependencies": {
    "aws-ssm": "^0.3.13",
    "common-types": "^1.23.1",
    "inferred-types": "^0.3.0",
    "native-dash": "^1.12.0"
  }
}
