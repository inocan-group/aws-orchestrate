#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/esprima@4.0.1/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/node_modules:/Volumes/Coding/inocan/open-source/node_modules:/Volumes/Coding/inocan/node_modules:/Volumes/Coding/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/js-yaml@3.14.1/node_modules/js-yaml/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/js-yaml@3.14.1/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esparse.js" "$@"
fi
