#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/typescript@4.2.4/node_modules/typescript/bin/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/typescript@4.2.4/node_modules/typescript/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/typescript@4.2.4/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/node_modules:/Volumes/Coding/inocan/open-source/node_modules:/Volumes/Coding/inocan/node_modules:/Volumes/Coding/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/ts-node@9.1.1_typescript@4.2.4/node_modules/ts-node/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/ts-node@9.1.1_typescript@4.2.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.2.4/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.2.4/node_modules/typescript/bin/tsserver" "$@"
fi
