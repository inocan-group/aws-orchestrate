#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/bin/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/crc-32@1.2.0/node_modules/crc-32/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/crc-32@1.2.0/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/node_modules:/Volumes/Coding/inocan/open-source/node_modules:/Volumes/Coding/inocan/node_modules:/Volumes/Coding/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/crc32-stream@4.0.2/node_modules/crc32-stream/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/crc32-stream@4.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../crc-32@1.2.0/node_modules/crc-32/bin/crc32.njs" "$@"
else
  exec node  "$basedir/../../../../../crc-32@1.2.0/node_modules/crc-32/bin/crc32.njs" "$@"
fi
