#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/bin/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/shelljs@0.8.4/node_modules/shelljs/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/shelljs@0.8.4/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/node_modules:/Volumes/Coding/inocan/open-source/node_modules:/Volumes/Coding/inocan/node_modules:/Volumes/Coding/node_modules:/Volumes/node_modules:/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/async-shelljs@0.1.4/node_modules/async-shelljs/node_modules:/Volumes/Coding/inocan/open-source/aws-orchestrate/common/temp/node_modules/.pnpm/async-shelljs@0.1.4/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../shelljs@0.8.4/node_modules/shelljs/bin/shjs" "$@"
else
  exec node  "$basedir/../../../../../shelljs@0.8.4/node_modules/shelljs/bin/shjs" "$@"
fi
