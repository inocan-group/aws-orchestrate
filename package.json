{
  "name": "aws-orchestrate",
  "description": "This library is intended to help projects intending to build a \"micro-service architecture\" to achieve those ambitions with some handy orchestration classes and types.",
  "version": "0.18.0",
  "homepage": "https://github.com/inocan-group/aws-orchestrate#readme",
  "bugs": {
    "url": "https://github.com/inocan-group/aws-orchestrate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inocan-group/aws-orchestrate.git"
  },
  "license": "MIT",
  "author": "Ken Snyder<ken@ken.net>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    "./devops": {
      "import": "./dist/devops/index.mjs",
      "require": "./dist/devops/index.js"
    },
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "bin": {
    "tsls": "./bin/tsls.js"
  },
  "scripts": {
    "autoindex": "npx dd autoindex",
    "clean": "rimraf dist src/**/*.mjs src/**/*.js",
    "change": "npx changeset ",
    "status": "npx changeset status",
    "version": "npx changeset version",
    "build": "run-s clean build:bundle",
    "build:bundle": "run-p build:wrapper build:devops build:cli",
    "build:cli": "npx tsup src/cli/tsls.ts --format=esm,cjs --minify --out-dir bin",
    "build:wrapper": "npx tsup src/wrapper-fn/index.ts --format=esm,cjs --minify --dts",
    "build:devops": "npx tsup src/devops/index.ts --format=esm,cjs --minify --dts --out-dir dist/devops",
    "circular": "yarn madge --circular src/**/*.ts",
    "docs": "node_modules/.bin/vuepress dev docs",
    "docs:build": "node_modules/.bin/vuepress build docs",
    "lint": "run-s lint:src lint:test lint:tsc",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:test": "eslint test/**/*.ts --fix",
    "lint:tsc": "tsc --noEmit",
    "test": "jest",
    "visualize": "yarn madge src --ts-config tsconfig.json --extensions ts -i /tmp/dep-visualize.png && open /tmp/dep-visualize.png",
    "watch": "run-p watch:wrapper watch:devops",
    "watch:wrapper": "npx tsup src/index.ts --format=esm,cjs --minify --dts --watch",
    "watch:cli": "npx tsup src/cli/tls.ts --format=esm,cjs --minify --watch",
    "watch:devops": "npx tsup src/devops/index.ts --format=esm,cjs --minify --dts --out-dir dist/devops --watch"
  },
  "dependencies": {
    "brilliant-errors": "^0.4.3",
    "cdk-iam-actions": "^1.0.0",
    "common-types": "^1.31.0",
    "destr": "^1.1.0",
    "globby": "^11.0.4",
    "ts-morph": "^13.0.2"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@type-challenges/utils": "^0.1.1",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/mermaid": "^8.2.7",
    "@types/node": "16",
    "@types/rimraf": "^3.0.2",
    "@types/test-console": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-last-updated": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "@vuepress/plugin-pwa": "^1.8.2",
    "async-shelljs": "^0.1.4",
    "aws-log": "^0.11.2",
    "aws-sdk": "^2.1043.0",
    "aws-ssm": "^0.4.0",
    "aws-xray-sdk-core": "^3.3.4",
    "dd": "^0.15.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "flatted": "^3.2.4",
    "jest": "^27.4.3",
    "jest-extended": "^1.2.0",
    "js-yaml": "^4.1.0",
    "madge": "^5.0.1",
    "mermaid": "^8.13.4",
    "native-dash": "^1.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "serverless": "^2.68.0",
    "simple-git": "^2.48.0",
    "test-console": "^2.0.0",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "tsup": "^5.10.1",
    "typescript": "^4.6.0-dev.20211205",
    "typescript-transform-paths": "^3.3.1",
    "vuepress": "^1.8.2",
    "vuepress-plugin-autometa": "^0.1.13",
    "vuepress-plugin-mermaidjs": "^1.8.1"
  },
  "peerDependencies": {
    "aws-log": "^0.11.2",
    "aws-ssm": "^0.4.0",
    "common-types": "^1.30.0",
    "native-dash": "^1.21.0",
    "simple-git": "^2.48.0"
  }
}
